def max_2_sum (arr)
	max = 0
	sec_max = 0  

	if arr.length == 0
		return 0
	end
	if arr.length == 1
		return arr[0]
	end
	
	sec_max = arr.sort[-2]
	
	max = arr.sort[-1]
	
	sum = max + sec_max	it 
	return sum

end


def sum_to_n?(arr, n)
	x = arr.combination(2)
	x.to_a.select {|param| param.inject(:+) == n}.length > 0
end
